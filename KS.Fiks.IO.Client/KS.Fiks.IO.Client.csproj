<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>KS FIKS IO Client</Title>
        <Authors>Ks-Kommunesektorens Organisasjon</Authors>
        <Copyright>Ks-Kommunesektorens Organisasjon</Copyright>
        <RootNamespace>KS.Fiks.IO.Client</RootNamespace>
        <PackageProjectUrl>https://github.com/ks-no/fiks-io-client-dotnet</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/ks-no/fiks-io-client-dotnet/raw/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/ks-no/fiks-io-client-dotnet.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>FIKS</PackageTags>
        <VersionPrefix>1.1.7</VersionPrefix>
        <PackageIconUrl>https://ks-no.github.io/images/favicon.png</PackageIconUrl>
        <TargetFrameworks>netstandard2.1;netcoreapp3.1</TargetFrameworks>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="KS.Fiks.ASiC-E" Version="1.0.1" />
        <PackageReference Include="KS.Fiks.Crypto" Version="1.0.0" />
        <PackageReference Include="KS.Fiks.IO.Send.Client" Version="1.0.4" />
        <PackageReference Include="KS.Fiks.Maskinporten.Client" Version="1.0.0" />
        <PackageReference Include="KS.Fiks.QA" Version="1.0.0" PrivateAssets="All" />
        <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
        <PackageReference Include="RabbitMQ.Client" Version="5.1.0" />
        <PackageReference Include="StyleCop.Analyzers" Version="1.0.2">
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
          <PrivateAssets>all</PrivateAssets>
        </PackageReference>
    </ItemGroup>

</Project>
